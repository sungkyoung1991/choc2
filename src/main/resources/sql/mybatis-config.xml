<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
 		 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 		"http://mybatis.org/dtd/mybatis-3-config.dtd">
  						
<configuration>

	<settings>
		<setting name="callSettersOnNulls" value="false"/>
	</settings>	
	
	<typeAliases>
		<typeAlias alias="user" type="com.sample.choc2.service.domain.User" />
		<typeAlias alias="boardVO" type="com.sample.choc2.service.domain.BoardVO" />
		<typeAlias alias="replyVO" type="com.sample.choc2.service.domain.ReplyVO" />
		<typeAlias alias="cosmetic" type="com.sample.choc2.service.domain.CosmeticVO" />
		<typeAlias alias="productVO" type="com.sample.choc2.service.domain.ProductVO" />
		<typeAlias alias="search" type="com.sample.choc2.common.Search" />
	</typeAliases>
	
	<mappers>
		<mapper resource="sql/UserMapper.xml"/>
		<mapper resource="sql/AdminMapper.xml"/>
		<mapper resource="sql/ProductMapper.xml"/>
		<mapper resource="sql/ReplyMapper.xml"/>
	</mappers>

</configuration>